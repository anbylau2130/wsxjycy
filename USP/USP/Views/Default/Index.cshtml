    function login() {
        var uname = $.trim($("#uname").val());
        var pwd = $.trim($("#pwd").val());
        var yzm = $.trim($("#yzm").val());
        if (uname == '' || pwd == '') {
            mylayer('请输入用户名和密码！');
            return;
        }
        if (yzm == '') {
            mylayer('请输入验证码！');
            return;
        }
        $.post("/User/Login", { Name: uname, Password: pwd, Captcha: yzm }, function (data) {
            if (data.flag == true) {
                $("#memberInfo").html(data.message);
                isLog = 'True';
            } else {
                mylayer(data.msg);
                $('#img_yzm').attr('src', "/Captcha/Index?d=" + Math.random());
            }
        })
    }
