    function tzyws(ty) {
        if (isLog != 'True') {
            layer.alert('请先登录');
        } else {
            $.post("/web/memberauthority/tzyw", { kw: ty }, function (data) {
                if (data.state == 'success') {
                    //window.open(data.msg);
                    window.open("/cn/ExpUrl.html?bgurl=" + encodeURIComponent(data.msg));
                } else {
                    layer.alert(data.msg);
                }
            })
        }
    }
