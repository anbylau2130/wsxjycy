             formatDate();
            function formatDate(){
                var obj;
                var newDate = new Date();
                var year,month,date,hours,minutes,seconds,day,time;
                var arrtDay = ['星期一','星期二','星期三','星期四','星期五','星期六','星期日',];
                year = newDate.getFullYear() + '年';
                month = newDate.getMonth() + 1 + '月';
                date = newDate.getDate() + '日';
                hours = newDate.getHours();
                minutes = newDate.getMinutes();
                seconds = newDate.getSeconds();
                hours = formatTime(hours);
                minutes = formatTime(minutes);
                seconds = formatTime(seconds);
                day = arrtDay[newDate.getDay() - 1];
                time = year + month + date + ' ' + day;
                obj = document.getElementById('showTime');
                obj.innerHTML = time;
                function formatTime(time){
                    if(time < 10){
                        return time = '0' + time;
                    }
                    return time;
                }
                    //setInterval(formatDate,1000);
            };
    });
    function enterShow(){
         l.find("div").show(300);
    }
